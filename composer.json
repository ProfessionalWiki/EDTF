{
	"name": "professional-wiki/edtf",
	"license": "proprietary",
	"description": "EDTF PHP is a small library for parsing, representing and working with the Extended Date/Time Format specification",
	"require": {
		"php": "^7.4|^8",
		"symfony/polyfill-php80": "^1.18.1"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.3",
		"vimeo/psalm": "^4.0.1",
		"phpstan/phpstan": "^0.12.50"
	},
	"autoload": {
		"psr-4": {
			"EDTF\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"EDTF\\Tests\\": "tests/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "0.1.x-dev"
		}
	}
}
